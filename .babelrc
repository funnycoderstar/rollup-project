{
    "presets": [
        "stage-0",
        [
            "env",
            {
                "targets": {
                    "browsers": ["last 3 versions"]
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties"
    ],
    "comments": false,
    "env": {
        "test": {
            "presets": [
                [
                    "env",
                    {
                        "modules": "commonjs"
                    }
                ]
            ]
        }
    }
}